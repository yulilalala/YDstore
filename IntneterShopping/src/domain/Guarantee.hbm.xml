<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="domain.Guarantee" table="guarantee">
		<id name="guarantee_id" column="guarantee_id">
			<generator class="native"/>
		</id>
		<property name="guarantee_name"></property>
		<property name="guarantee_price"></property>
	</class>
</hibernate-mapping>