<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="domain.Spu_options" table="spu_options">
		<id name="spu_options_id" column="spu_options_id">
			<generator class="native"></generator>
		</id>
		<property name="spu_id"></property>
		<property name="specification_option_id"></property>
		<property name="specification_id"></property>
		<property name="specification_group_id"></property>
	</class>
</hibernate-mapping>